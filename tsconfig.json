{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["DOM", "ES2022", "DOM.Iterable"],
		"module": "ESNext",
		"jsx": "react-jsx",
		"strict": true,
		"noEmit": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"types": ["@rsbuild/core/types"]
	},
	"include": ["index.tsx"]
}
